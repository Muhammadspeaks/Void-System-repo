
#Browser

super + w
        firefox
        
#Existing linux (Shutdown, reboot, exit)

super + shift + BackSpace
        $HOME/bin/exit.sh

# Volume controls

super + KP_Divide
        bash $HOME/bin/notify-vol.sh unmute

super + KP_Multiply
        bash $HOME/bin/notify-vol.sh mute

super + KP_Add   
        bash $HOME/bin/notify-vol.sh up

super + KP_Subtract 
        bash $HOME/bin/notify-vol.sh down
# Dmenu

super + shift + d
        dmenu_run


# Screen-shot (flameshot)

super + shift + s
        flameshot full
